{
	"Logging": {
		"LogLevel": {
			"Default": "Information",
			"Microsoft": "Warning",
			"Microsoft.Hosting.Lifetime": "Information"
		}
	},
	"AllowedHosts": "*",
	"ConnectionStrings": {
		"DefaultConnection": "Server=HOME-PC;Database=websocketsdb;Trusted_Connection=True;"
	},
	"JwtAuthOptions": {
		"SecretKey": "MySuperSecretJwtTokenKey",
		"ExpiresHours": "2"
	},
	"Cors": {
		"Origin": "http://localhost:5000"
	},
	"Quartz": {
		"QuartzCronExpression": "0/5 * * * * ?"
	}

}
